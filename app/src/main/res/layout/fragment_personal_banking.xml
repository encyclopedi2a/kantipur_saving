<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.education.educatenepal.activity.kantipursavingandcredit.fragments.AboutFragment">

    <com.sudeep23.lollipoptabs.fixedtab.FixedTabLayout
        android:id="@+id/fixed_tab"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:elevation="5dp"
        android:theme="@style/InnerSlidingTabStyle" />

    <com.education.educatenepal.activity.kantipursavingandcredit.classes.CustomInnnerViewPager
        android:id="@+id/viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>
